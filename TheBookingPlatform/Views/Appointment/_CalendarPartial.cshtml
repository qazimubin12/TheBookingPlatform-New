                                    // Show loading spinner before the AJAX request is sent
                                    showLoadingSpinner();
                                },
                                success: function (response) {
                                    debugger
                                    if (response.success) {
                                        // Event updated successfully
                                        showSuccessAlert();
                                    } else {
                                        // Handle error if the update fails
                                        revertFunc(); // Revert the event to its original position
                                    }
                                },
                                error: function (error) {
                                    // Handle error if the AJAX request fails
                                    console.error('Error updating event:', error);
                                    revertFunc(); // Revert the event to its original position
                                },
                                complete: function () {
                                    // Hide loading spinner once the AJAX request is completed
                                    hideLoadingSpinner();

                                }
                            });

                        }
                        else {
                            // If the user cancels the update, revert the event to its original position
                            revertFunc();
                        }
                    }
                    else {
                        revertFunc();

                    }
                }

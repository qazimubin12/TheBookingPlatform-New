                                var services = appointment.Services.map(s => "- " + s.Name);
                                var serviceIDs = appointment.Services.map(s => s.ID, ",");
                                var serviceList = services.join(",");
                                var title = appointment.CustomerFirstName + " " + appointment.CustomerLastName;
                                var eventId = appointment.ID;
                                var FromGCAL = appointment.FromGCAL;
                                var customer = FromGCAL
                                    ? "Appointment from Google Calendar"
                                    : appointment.Services.some(x => x.Category === "ABSENSE") ? "Walk In" : title;
                                var blockedEvent = appointment.BlockedEvent;                                    
                                var customerPhone = appointment.MobileNumber || "";
                                var notes = appointment.Notes ? appointment.Notes.replace("\r\n", "").replace("\n", " ") : "";
                                var startDate = parseDotNetDate(appointment.Date);
                                var startTime = parseDotNetDate(appointment.Time);
                                startDate.setHours(startTime.getHours(), startTime.getMinutes(), startTime.getSeconds());
                                var endDate = addMinutes(startDate, appointment.TotalDuration);
                                var haveBuffer = appointment.Buffers.length > 0 ? "true" : "false";


                                var bufferList = [];

                                // Check if there are buffers in the appointment object
                                if (appointment.Buffers.length > 0) {
                                    let lastBufferEndTime = null; // To store the last buffer's EndTime

                                    appointment.Buffers.forEach(function (buffer) {
                                        var bufferStart = parseDotNetDate(buffer.Date);
                                        var bufferStartTime = parseDotNetDate(buffer.Time);
                                        bufferStart.setHours(bufferStartTime.getHours(), bufferStartTime.getMinutes(), bufferStartTime.getSeconds());

                                        var bufferEndTime = parseDotNetDate(buffer.EndTime);
                                        var bufferEnd = new Date(bufferStart); // Clone the start date
                                        bufferEnd.setHours(bufferEndTime.getHours(), bufferEndTime.getMinutes(), bufferEndTime.getSeconds());

                                        // Store the last buffer's end time
                                        lastBufferEndTime = bufferEndTime;

                                        var bufferDesc = buffer.Description;
                                        var serviceIdforbuffer = buffer.ServiceID;

                                        // Push the buffer details to the list
                                        bufferList.push({
                                            ID: buffer.ID,
                                            start: bufferStart,
                                            end: bufferEnd,
                                            description: bufferDesc,
                                            ServiceID: serviceIdforbuffer
                                        });
                                    });

                                    // Set endDate after the loop with the last buffer's EndTime
                                    if (lastBufferEndTime) {
                                        endDate.setHours(lastBufferEndTime.getHours(), lastBufferEndTime.getMinutes(), lastBufferEndTime.getSeconds());
                                    }
                                }
                                events.push({
                                    id: eventId,
                                    title: title,
                                    blockedEvent: blockedEvent,
                                    content: serviceList,
                                    customer: customer,
                                    customerphone: customerPhone,
                                    start: startDate,
                                    reminderSent: appointment.ReminderSent,
                                    end: endDate,
                                    editable: !FromGCAL,
                                    description: serviceList,
                                    notes: notes,
